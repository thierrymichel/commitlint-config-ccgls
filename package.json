{
  "name": "commitlint-config-ccgls",
  "version": "1.4.1",
  "description": "Commitlint companion for cz-ccgls.",
  "keywords": [
    "commitlint",
    "commit",
    "linter",
    "commitizen",
    "conventional",
    "changelog",
    "gitmoji",
    "lerna",
    "scope"
  ],
  "main": "index.js",
  "files": [
    "index.js",
    "parser-preset.js"
  ],
  "scripts": {
    "commit": "npx git-cz || exit 0",
    "commit-retry": "npx git-cz --retry || exit 0",
    "lint": "eslint *.js",
    "semantic-release": "semantic-release",
    "test": "echo 'Tests need to be setup!'"
  },
  "homepage": "https://github.com/thierrymichel/commitlint-config-ccgls#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/thierrymichel/commitlint-config-ccgls.git"
  },
  "author": {
    "name": "Thierry Michel",
    "email": "thierry@epic.net",
    "url": "http://epic.net"
  },
  "bugs": {
    "url": "https://github.com/thierrymichel/commitlint-config-ccgls/issues"
  },
  "license": "SEE LICENSE IN UNLICENSE",
  "dependencies": {
    "@commitlint/config-conventional": "^14.1.0",
    "@lerna/project": "^4.0.0",
    "emoji-regex": "^10.2.1",
    "import-from": "^4.0.0",
    "read-pkg-up": "^7.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^14.1.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "commitizen": "^4.2.4",
    "cz-ccgls": "0.5.0",
    "eslint": "^8.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "lerna": "^4.0.0",
    "lint-staged": "^11.2.6",
    "prettier": "^2.4.1",
    "semantic-release": "^18.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-ccgls"
    },
    "cz-ccgls": {
      "useScopes": false
    }
  },
  "packageManager": "yarn@3.1.0"
}
